<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/lista_feeds_principal"
    android:paddingBottom="@dimen/espacamento_padrao_item_listas"
    android:orientation="vertical">
	
		<TextView
		    android:id="@+id/nome_feed"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/nome_do_feed"
		    android:textSize="22sp" />

		<TextView
		    android:id="@+id/url_feed"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/uri_do_feed"
		    android:textSize="12sp" />
</LinearLayout>
